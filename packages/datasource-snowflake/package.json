{
  "name": "@smallwebco/tinypivot-datasource-snowflake",
  "type": "module",
  "version": "1.0.64",
  "description": "TinyPivot Snowflake Datasource - Snowflake data warehouse connector for TinyPivot Studio",
  "author": "Small Web, LLC",
  "license": "SEE LICENSE IN LICENSE.md",
  "homepage": "https://github.com/Small-Web-Co/tinypivot",
  "repository": {
