{
  "name": "tinypivot",
  "version": "1.0.4",
  "description": "A powerful Excel-like data grid and pivot table component for Vue 3 by Small Web, LLC",
  "type": "module",
  "main": "./dist/tinypivot.umd.cjs",
  "module": "./dist/tinypivot.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/tinypivot.js",
      "require": "./dist/tinypivot.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:lib": "vite build --mode lib",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "demo": "vite --config vite.demo.config.ts",
    "build:demo": "vite build --config vite.demo.config.ts",
    "release": "vite build --mode lib && git add -A && git commit -m \"chore: release v$(node -p \"require('./package.json').version\")\" && git tag v$(node -p \"require('./package.json').version\") && git push origin master --tags && pnpm publish --access public --no-git-checks"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "dependencies": {
    "@tanstack/vue-table": "^8.20.0"
  },
  "devDependencies": {
    "@vercel/node": "^3.0.0",
    "stripe": "^17.0.0",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.2",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0",
    "vue": "^3.3.8",
    "vue-tsc": "^1.8.22"
  },
  "keywords": [
    "vue",
    "vue3",
    "datagrid",
    "data-grid",
    "excel",
    "spreadsheet",
    "pivot-table",
    "table",
    "grid",
    "filtering",
    "sorting",
    "aggregation"
  ],
  "author": "Small Web, LLC",
  "license": "SEE LICENSE IN LICENSE.md",
  "homepage": "https://github.com/Small-Web-Co/tinypivot",
  "repository": {
    "type": "git",
    "url": "https://github.com/Small-Web-Co/tinypivot.git"
  },
  "bugs": {
    "url": "https://github.com/Small-Web-Co/tinypivot/issues"
  },
  "packageManager": "pnpm@9.0.0+sha512.b4106707c7225b1748b61595953ccbebff97b54ad05d002aa3635f633b9c53cd666f7ce9b8bc44704f1fa048b9a49b55371ab2d9e9d667d1efe2ef1514bcd513"
}
